<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#037748"
    android:backgroundTint="#2E4340"
    tools:context=".Bilan">

    <TextView
        android:id="@+id/textView14"
        android:layout_width="127dp"
        android:layout_height="37dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="10dp"
        android:gravity="start|center_vertical"
        android:text="@string/check_mark"
        android:textAlignment="viewStart"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="127dp"
        android:layout_height="37dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="10dp"
        android:gravity="start|center_vertical"
        android:text="@string/physical_mark"
        android:textAlignment="viewStart"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="119dp"
        android:layout_height="0dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="10dp"
        android:gravity="start|center_vertical"
        android:text="@string/sexe"
        android:textAlignment="viewStart"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextView2" />

    <TextView
        android:id="@+id/TextView2"
        android:layout_width="124dp"
        android:layout_height="26dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="13dp"
        android:gravity="center_vertical"
        android:text="@string/name"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="127dp"
        android:layout_height="37dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="10dp"
        android:gravity="start|center_vertical"
        android:text="@string/heart_mark"
        android:textAlignment="viewStart"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="125dp"
        android:layout_height="29dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="10dp"
        android:gravity="start|center_vertical"
        android:text="@string/age"
        android:textAlignment="viewStart"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="118dp"
        android:layout_height="39dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="124dp"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="118dp"
        android:layout_height="39dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="124dp"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView35" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="119dp"
        android:layout_height="32dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="124dp"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView32" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="126dp"
        android:layout_height="122dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="4dp"
        android:background="#F19D9D"
        android:foregroundTint="#1A237E"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bilan_de_sante"
        android:contentDescription="@string/result" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="120dp"
        android:layout_height="32dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="124dp"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView33" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="121dp"
        android:layout_height="26dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="124dp"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="119dp"
        android:layout_height="22dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="124dp"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView31" />

    <Button
        android:id="@+id/send1"
        android:layout_width="198dp"
        android:layout_height="40dp"
        android:layout_marginStart="112dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#B17866"
        android:onClick="onClick"
        android:text="@string/share"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/button4"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/send2"
        android:layout_width="198dp"
        android:layout_height="40dp"
        android:layout_marginStart="112dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#B17866"
        android:onClick="writeToFile"
        android:text="DOWNLOAD"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/send1"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/button5"
        android:layout_width="198dp"
        android:layout_height="40dp"
        android:layout_marginStart="112dp"
        android:layout_marginBottom="5dp"
        android:backgroundTint="#B17866"
        android:onClick="action_Exit"
        android:text="@string/exit"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button4"
        android:layout_width="198dp"
        android:layout_height="40dp"
        android:layout_marginStart="112dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#B17866"
        android:onClick="goHome"
        android:text="@string/home"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintStart_toStartOf="parent" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="102dp"
        android:layout_height="67dp"
        android:layout_marginEnd="152dp"
        android:layout_marginBottom="4dp"
        android:contentDescription="@string/lienweb"
        android:onClick="action_Website"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/unnamed" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-85dp"
        android:layout_marginBottom="4dp"
        android:text="@string/click_here"
        android:textColor="#FBFBFB"
        app:layout_constraintBottom_toTopOf="@+id/imageView4"
        app:layout_constraintStart_toEndOf="@+id/imageView4" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="32dp"
        android:text="@string/for_more_information"
        android:textColor="#FBFBFB"
        app:layout_constraintBottom_toTopOf="@+id/send2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="118dp"
        android:layout_height="51dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="148dp"
        android:gravity="center|center_horizontal"
        android:text="@string/result"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageButton
        android:id="@+id/info1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="304dp"
        android:backgroundTint="#2E4340"
        android:onClick="showInfo1"
        app:layout_constraintStart_toEndOf="@+id/textView34"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_info_details"
        android:contentDescription="@string/for_more_information" />

    <ImageButton
        android:id="@+id/info2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="352dp"
        android:backgroundTint="#2E4340"
        android:onClick="showInfo2"
        app:layout_constraintStart_toEndOf="@+id/textView34"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_info_details"
        android:contentDescription="@string/for_more_information"/>

    <ImageButton
        android:id="@+id/info3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="400dp"
        android:backgroundTint="#2E4340"
        android:onClick="showInfo3"
        app:layout_constraintStart_toEndOf="@+id/textView34"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_info_details"
        />

</androidx.constraintlayout.widget.ConstraintLayout>