<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#037748"
    android:backgroundTint="#2E4340"
    tools:context=".Bilan">

    <TextView
        android:id="@+id/TextView2"
        android:layout_width="120dp"
        android:layout_height="26dp"
        android:layout_marginStart="184dp"
        android:layout_marginTop="68dp"
        android:gravity="center_vertical"
        android:text="@string/name"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="120dp"
        android:layout_height="26dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="36dp"
        android:gravity="start|center_vertical"
        android:text="@string/heart_mark"
        android:textAlignment="viewStart"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="120dp"
        android:layout_height="26dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="36dp"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/textView33" />

    <ImageButton
        android:id="@+id/info1"
        android:layout_width="50dp"
        android:layout_height="26dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="36dp"
        android:backgroundTint="#2E4340"
        android:contentDescription="@string/for_more_information"
        android:onClick="showInfo1"
        app:layout_constraintStart_toEndOf="@+id/textView34"
        app:layout_constraintTop_toBottomOf="@+id/textView33"
        app:srcCompat="@android:drawable/ic_menu_info_details" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="120dp"
        android:layout_height="26dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView16"
        app:layout_constraintTop_toBottomOf="@+id/textView35" />

    <ImageButton
        android:id="@+id/info2"
        android:layout_width="50dp"
        android:layout_height="26dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        android:backgroundTint="#2E4340"
        android:contentDescription="@string/for_more_information"
        android:onClick="showInfo2"
        app:layout_constraintStart_toEndOf="@+id/textView35"
        app:layout_constraintTop_toBottomOf="@+id/info1"
        app:srcCompat="@android:drawable/ic_menu_info_details" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="120dp"
        android:layout_height="26dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="8dp"
        android:gravity="start|center_vertical"
        android:text="@string/check_mark"
        android:textAlignment="viewStart"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="120dp"
        android:layout_height="26dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="118dp"
        android:layout_height="46dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="8dp"
        android:gravity="start|center_vertical"
        android:text="@string/physical_mark"
        android:textAlignment="viewStart"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <ImageButton
        android:id="@+id/info3"
        android:layout_width="50dp"
        android:layout_height="26dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#2E4340"
        android:onClick="showInfo3"
        app:layout_constraintStart_toEndOf="@+id/textView36"
        app:layout_constraintTop_toBottomOf="@+id/info2"
        app:srcCompat="@android:drawable/ic_menu_info_details" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="120dp"
        android:layout_height="26dp"
        android:layout_marginStart="184dp"
        android:layout_marginTop="4dp"
        android:gravity="start|center_vertical"
        android:text="@string/age"
        android:textAlignment="viewStart"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="120dp"
        android:layout_height="26dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="69dp"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/TextView2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="120dp"
        android:layout_height="26dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView31" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="120dp"
        android:layout_height="26dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView32" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="120dp"
        android:layout_height="26dp"
        android:layout_marginStart="184dp"
        android:layout_marginTop="4dp"
        android:gravity="start|center_vertical"
        android:text="@string/sexe"
        android:textAlignment="viewStart"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextView2" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="73dp"
        android:layout_height="63dp"
        android:background="#F19D9D"
        android:contentDescription="@string/result"
        android:foregroundTint="#1A237E"
        app:srcCompat="@drawable/bilan_de_sante"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="6dp" />

    <Button
        android:id="@+id/send2"
        android:layout_width="198dp"
        android:layout_height="40dp"
        android:layout_marginEnd="33dp"
        android:backgroundTint="#B17866"
        android:onClick="writeToFile"
        android:text="DOWNLOAD"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/send1"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/button4"
        android:layout_width="198dp"
        android:layout_height="40dp"
        android:layout_marginEnd="33dp"
        android:backgroundTint="#B17866"
        android:onClick="goHome"
        android:text="@string/home"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="198dp"
        android:layout_height="40dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="24dp"
        android:backgroundTint="#B17866"
        android:onClick="action_Exit"
        android:text="@string/exit"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/send1"
        android:layout_width="198dp"
        android:layout_height="40dp"
        android:layout_marginEnd="33dp"
        android:backgroundTint="#B17866"
        android:onClick="onClick"
        android:text="@string/share"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/button4"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="OnClick" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="102dp"
        android:layout_height="67dp"
        android:layout_marginEnd="84dp"
        android:contentDescription="@string/lienweb"
        android:onClick="action_Website"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/unnamed" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="67dp"
        android:layout_marginBottom="32dp"
        android:text="@string/for_more_information"
        android:textColor="#FBFBFB"
        app:layout_constraintBottom_toTopOf="@+id/send2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="103dp"
        android:text="@string/click_here"
        android:textColor="#FBFBFB"
        app:layout_constraintBottom_toTopOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="104dp"
        android:layout_height="37dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="314dp"
        android:gravity="center|center_horizontal"
        android:text="@string/result"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.194"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>